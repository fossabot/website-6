{
  "name": "notatweet",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aphrodite": "^2.2.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-links": "^3.4.10",
    "@storybook/addons": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "@types/react": "^16.4.18",
    "@types/storybook__react": "^3.0.9",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "react-scripts": "1.1.4",
    "sw-precache": "^5.2.1"
  },
  "proxy": {
    "/auth/twitter": {
      "target": "http://localhost:7000"
    },
    "/identity": {
      "target": "http://localhost:7000"
    }
  },
  "scripts": {
